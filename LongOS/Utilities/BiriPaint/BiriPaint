local function run()
	shell.run('/LongOS/Utilities/BiriPaint/BiriPaintNewImage.lua');
	shell.run('/LongOS/Utilities/BiriPaint/BiriPaintWindow.lua');
	local application = Application('BiriPaint', false, true);
	local biriPaintWindow = BiriPaintWindow(application);
	application:Run(biriPaintWindow);
end

System:Try(run);